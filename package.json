{
  "name": "@ktsstudio/mediaproject-vk",
  "version": "2.0.0",
  "description": "Package with utils for VK Mini Apps",
  "author": "KTS Studio <hello@ktsstudio.ru> (https://kts.studio)",
  "keywords": [
    "vk",
    "vkontakte",
    "vk-mini-app",
    "vk-bridge"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ktsstudio/mediaproject-vk.git"
  },
  "bugs": {
    "email": "hello@ktsstudio.ru",
    "url": "https://github.com/ktsstudio/mediaproject-vk/issues"
  },
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "browser": "./dist/es/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "logo.png"
  ],
  "scripts": {
    "rm-build": "rimraf dist",
    "dev": "rollup -c -w",
    "lint": "eslint src --fix",
    "codestyle": "prettier --write \"src/**/*.{ts,tsx,js,jsx,scss}\"",
    "tsc-check": "tsc --skipLibCheck --noEmit --project tsconfig.json",
    "build-ts": "rm -rf dist && NODE_ENV=production rollup -c",
    "build-styles": "cpx \"src/**/*.*css\" dist",
    "build": "yarn run rm-build && yarn run build-ts && yarn run build-styles",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,js}?(x)": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@ktsstudio/mediaproject-utils": "^4.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^16.4.0",
    "@types/react": "^17.0.2",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vkontakte/vk-bridge": "^2.7.2",
    "cpx": "^1.5.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "rollup": "^3.10.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "styled-components": "^5.3.3",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@ktsstudio/mediaproject-utils": "^4.1.0",
    "@types/react": "^17.0.0 || ^18.0.0",
    "@types/styled-components": "^5.1.26",
    "@vkontakte/vk-bridge": "^2.7.2",
    "@vkontakte/vkui": "~5.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "styled-components": "^5.3.1"
  }
}
